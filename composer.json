{
    "name": "serviceinfoadn/yalinqo",
    "description": "YaLinqo, a LINQ-to-objects library for PHP",
    "license": "BSD-2-Clause",
    "keywords": ["linq", "linqo", "query", "statistic"],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "phpdocumentor/phpdocumentor": "^2.8",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "YaLinqo\\": "YaLinqo/"
        },
        "files": [
            "YaLinqo/Linq.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YaLinqo\\Tests\\": "Tests/"
        }
    }
}